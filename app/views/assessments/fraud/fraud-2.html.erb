<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <%= form_tag("#{request.fullpath}", method: "post") do %>
      <div class="govuk-form-group">
        <div class="govuk-caption-l">Fraud checks</div >
        <h1 class="govuk-heading-l">How many counter-fraud data sources do you check with?</h1>
          <div class="govuk-radios">
            <% @form.lists['counter_fraud_data_sources'].items.each do |value, item| %>
              <%= render 'shared/radio_item', name: :counter_fraud_data_sources, item: item %>
            <% end %>
          </div>
        </fieldset>
      </div>
      <%= submit_tag("Continue", class: "govuk-button") %>
    <% end %>
  </div>
</div>