<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <%= form_tag("#{request.fullpath}", method: "post") do %>
      <div class="govuk-form-group">
        <div class="govuk-caption-l">Fraud checks</div >
        <h1 class="govuk-heading-l">What does the system do when it does the checks?</h1>
          <div class="govuk-radios">
            <% @form.lists['system_checks'].items.each do |value, item| %>
              <%= render 'shared/check_box_item', name: :system_checks, item: item %>
            <% end %>
          </div>
        </fieldset>
      </div>
      <%= submit_tag("Continue", class: "govuk-button") %>
    <% end %>
  </div>
</div>





